<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
	
	<!-- Session -->
	<sys:String x:Key="String/Session.Message.SourceNotFound.StandardOutput">找不到 '{0}' 以讀取日誌。請確認指令或可執行檔已安裝且其路徑已加入至檔案 '/etc/paths'。</sys:String>
	
	<!-- SessionView -->
	<sys:String x:Key="String/SessionView.AddLogFiles.WithShortcut">加入日誌檔案 (⌘O)</sys:String>
	<sys:String x:Key="String/SessionView.MemoryUsage">總記憶體使用量</sys:String>
	<sys:String x:Key="String/SessionView.PredefinedLogTextFilters">已定義的文字篩選 (⌘P)</sys:String>
	<sys:String x:Key="String/SessionView.RestartAsAdministrator">以 Superuser 身分重新啟動</sys:String>
	<sys:String x:Key="String/SessionView.ShowAllLogsTemporarily">暫時顯示所有日誌 (⌥A)</sys:String>
	<sys:String x:Key="String/SessionView.ShowFileInExplorer">在 Finder 中顯示檔案</sys:String>
	<sys:String x:Key="String/SessionView.ShowMarkedLogsTemporarily">暫時只顯示已標記的日誌 (⌥M)</sys:String>
	<sys:String x:Key="String/SessionView.ShowWorkingDirectoryInExplorer">在 Finder 中開啟工作目錄</sys:String>
	
</ResourceDictionary>
